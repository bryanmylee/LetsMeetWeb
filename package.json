{
  "name": "meetwhen-v2",
  "description": "A group scheduling application",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p watch:css watch:sapper",
    "watch:css": "postcss src/global.pcss -o static/global.css -w",
    "watch:sapper": "PORT=55000 sapper dev --dev-port=55100 --no-hot --ext '.svelte'",
    "build": "run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/global.pcss -o static/global.css",
    "build:sapper": "NODE_ENV=production sapper build --ext '.svelte'",
    "export": "run-s build:css export:sapper",
    "export:sapper": "sapper export --ext '.svelte'",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/chroma-js": "^2.1.3",
    "@types/compression": "^1.7.0",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^14.14.20",
    "@types/polka": "^0.5.1",
    "autoprefixer": "^10.2.0",
    "body-scroll-lock": "^3.1.5",
    "chroma-js": "^2.1.0",
    "cssnano": "^4.1.10",
    "dayjs": "^1.10.2",
    "dotenv": "^8.2.0",
    "file-loader": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash-es": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-nested": "^5.0.3",
    "sapper": "^0.28.0",
    "sapper-environment": "^1.0.1",
    "svelte": "^3.31.2",
    "svelte-check": "^1.1.25",
    "svelte-feather-icons": "^3.3.0",
    "svelte-loader": "^2.9.0",
    "svelte-popperjs": "^1.2.1",
    "svelte-preprocess": "^4.3.0",
    "tailwind-css-variables": "^2.0.3",
    "tailwindcss": "^2.0.2",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "tailwindcss-important": "^1.0.0",
    "tailwindcss-pseudo-elements": "^1.5.1",
    "tailwindcss-scroll-snap": "^1.0.3",
    "ts-loader": "^8.0.14",
    "tslib": "^2.1.0",
    "typescript": "^4.0.3",
    "vercel-sapper": "^0.43.0",
    "webpack": "^4.7.0",
    "webpack-modules": "^1.0.0"
  }
}
